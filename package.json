{
  "name": "makeuc-website",
  "version": "0.1.0",
  "description": "Website for MakeUC",
  "main": "index.js",
  "repository": "git@github.com:MakeUC/makeuc-website.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "run-p compile:client",
    "compile:client": "cd client && yarn compile",
    "dev": "run-p dev:client",
    "dev:client": "cd client && yarn dev",
    "lint": "run-p lint:client",
    "lint:client": "cd client && yarn lint",
    "lint-staged": "run-p lint-staged:client",
    "lint-staged:client": "cd client && yarn lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "husky": "^8.0.3"
  }
}
