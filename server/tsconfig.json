{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve"
  },
  "include": [
    "./.eslintrc.cjs",
    "admin/**/*.ts",
    "admin/**/*.tsx",
    "src/**/*.ts",
    "keystone.ts",
    "node_modules/.keystone/types.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}